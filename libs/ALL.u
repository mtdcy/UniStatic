# all pre-defined targets

upkg_type=PHONY

upkg_dep=(
    # zip
    gzip lzip unrar
    # utils
    coreutils gsed gawk grep gmake
    # shell
    bash
    # editor
    neovim
    # net
    wget iperf3
    # multimedia
    ffmpeg ffmpeg@4 mac
    # misc
    neofetch unrar
)

# not work with macOS
upkg_darwin || upkg_dep+=(
    bash@3.2
    bash@4.4
) || true

